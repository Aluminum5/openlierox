repeat = 10 
col_layer = -1 
invisible = 1

on ground_collision()
apply_map_effect( exp1hole.mfx )
 remove()

on detect_range( 6 , 6,0)
 damage ( 0.6 )
push ( 0.5 )

on detect_range( 6 , 6,[0,1,2,3,4])
 damage ( 0.3 )
push ( 0.5 )

on detect_range( 6 , 6,[7])
 damage ( 0.75 )

on timer( 420 )
remove()
