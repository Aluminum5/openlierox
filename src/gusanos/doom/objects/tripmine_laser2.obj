blender = add
alpha = 244
colour = [100,0,0]
repeat = 1000
col_layer = -1
line_to_origin = 1
wu_pixels = 1
bounce_factor = 0
ground_friction = 0

on ground_collision()
 remove()

on detect_range(2, 0)
 shoot_particles ( tripmine_laser3.obj , 1 , 1, 0, 0, 0, 0, 0, 0)
 remove()

on detect_range(2, 2,[0,2,3,4])
 shoot_particles ( tripmine_laser3.obj , 1 , 1, 0, 0, 0, 0, 0, 0)
 remove()